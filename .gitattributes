# Binary files with Git LFS
*.zip filter=lfs diff=lfs merge=lfs -text
*.cjs filter=lfs diff=lfs merge=lfs -text

# JavaScript files should use LF
*.js text eol=lf
*.json text eol=lf
*.yml text eol=lf

# Yarn specific files with Git LFS
/.yarn/releases/** filter=lfs diff=lfs merge=lfs -text
/.yarn/cache/** filter=lfs diff=lfs merge=lfs -text
/.pnp.cjs filter=lfs diff=lfs merge=lfs -text

# Bundle files with Git LFS
/bundles/**/*.js filter=lfs diff=lfs merge=lfs -text
